<?xml version="1.0" encoding="windows-1252"?>
<!--
    Arquivo de configuração para exportar os dados do TimeTables para o SUAP-EDU.
    Esse arquivo deve ser colocado no seguinte caminho: "C:/CAMINHO_PARA_O_TIMETABLES/template/xmlexport/"
 -->
<timetable importtype="database" options="idprefix:SUAP_EDU" displayname="SUAP EDU" displaycountries="br">
    <periods options="canadd,export:silent" columns="period,name,short,starttime,endtime"></periods>
    <cards options="import:disable,canadd,canremove,canupdate,primarytt,silent"
           columns="day,period,classids,subjectid,teacherids,classroomids"></cards>
</timetable>